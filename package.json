    "jest": "~26.6.2",
    "jest-environment-node": "~26.6.2",
    "nodemon": "~2.0.3",
    "parcel-bundler": "1.12.3",
    "shelljs": "~0.8.4"
  },
  "dependencies": {
    "near-api-js": "~0.35.0",
    "regenerator-runtime": "~0.13.5"
  }
